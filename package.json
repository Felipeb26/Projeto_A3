{
	"name": "projeto_a3",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "concurrently \"yarn main\" \"yarn gates\" \"yarn crud\" \"yarn mail\" \"yarn front\" \"yarn front-doc\"",
		"test": "concurrently \"yarn t-main\" \"yarn t-gates\" \"yarn t-crud\" \"yarn t-mail\" \"yarn front-doc\"",
		"front": "npm run ng serve --prefix page_login",
		"front-doc": "npm run compodoc:build-and-serve --prefix page_login",
		"main": "node server.js",
		"gates": "node Gateway/server.js",
		"crud": "node BACK/server.js",
		"mail": "node MAIL/server.js",
		"t-main": "nodemon server.js",
		"t-gates": "nodemon Gateway/server.js",
		"t-crud": "nodemon BACK/server.js",
		"t-mail": "nodemon MAIL/server.js"
	},
	"dependencies": {
		"concurrently": "^7.4.0",
		"nodemon": "^2.0.20"
	}
}
